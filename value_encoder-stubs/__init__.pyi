from typing import Sequence, overload

from numpy import int16
from numpy.typing import NDArray

class ValueEncoder:
    def __init__(self) -> None: ...
    @property
    def classes_(self) -> str: ...
    def fit(self, characters: str) -> ValueEncoder: ...
    @overload
    def transform(self, value: str, cap: bool = False) -> NDArray[int16]: ...
    @overload
    def transform(self, values: Sequence[str], missing_value: int = -1, cap: bool = False) -> NDArray[int16]: ...
    def inverse_transform(self, value: NDArray[int16]) -> str: ...
