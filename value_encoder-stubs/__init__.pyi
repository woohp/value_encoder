import numpy
from numpy import uint8


class ValueEncoder:
    def __init__(self: ValueEncoder) -> None: ...
    @property
    def classes_(self: ValueEncoder) -> str: ...
    def fit(self: ValueEncoder, characters: str) -> ValueEncoder: ...
    def transform(self: ValueEncoder, characters: str, cap: bool = False) -> numpy.ndarray[uint8]: ...
    def fit_transform(self: ValueEncoder, characters: str, cap: bool = False) -> numpy.ndarray[uint8]: ...
    def inverse_transform(self: ValueEncoder, value: numpy.ndarray[uint8]) -> str: ...
